{
	"name": "KGen",
	"description": "__MSG_description__",
	"version": "0.7",
	"icons": {
		"16": "icon-16.png",
		"32": "icon-32.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"permissions": [ "tabs", "http://*/*", "https://*/*"],
	"browser_action": {
		"default_icon": "icon-32.png",
		"default_title": "__MSG_tooltip__"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [ {
		"matches": [ "http://*/*", "https://*/*" ],
		"js": [ "mycontentscript.js" ],
		"run_at": "document_end"
	} ],
	"default_locale":"en",
	"manifest_version": 2
}
